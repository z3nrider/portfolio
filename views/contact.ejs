<!DOCTYPE html>
<html lang="en">
<head>
    <%- include('partials/headMeta') %>
    <title>Contact</title>
</head>
<body>
    <%- include('partials/header') %>

    <div class="container mt-4">
        <form>
            <div class="form-row">
              <div class="form-group col-md-6">
                <label for="inputName1">Name:</label>
                <input type="text" class="form-control" id="inputName1" placeholder="John Doe">
              </div>
              <div class="form-group col-md-6">
                <label for="inputEmail1">Email:</label>
                <input type="email" class="form-control" id="inputEmail1" placeholder="johndoe@example.com">
              </div>
              <div class="form-group col-md-6">
                <label for="comment">Comment:</label>
                <textarea class="form-control" rows="5" id="comment"></textarea>
              </div>
            </div>
            <div class="form-row formButton">
              <button type="submit" class="btn btn-outline-light btn-sm">Contact Brandon</button>
            </div>
          </form>
    </div>

</body>
<footer>
  <%- include('partials/footer') %>
</footer>
</html>